{
    "suggest.noselect": false,
    "suggest.minTriggerInputLength": 2,
    "coc.preferences.jumpCommand": "drop",
    "tsserver.implicitProjectConfig.experimentalDecorators": true,
    "explorer.keyMappingMode": "none",
    "explorer.file.autoReveal": false,
    "explorer.width": 30,
    "explorer.contentWidthType": "win-width",
    "explorer.file.root.template": "[icon] [title] [root]",
    "explorer.file.child.template": " [indent][icon | 1] [filename omitCenter 1][modified][readonly] [link growRight 1][git]",
    "explorer.filename.colored.enable": false,
    "explorer.icon.enableNerdfont": true,
    "explorer.git.icon.status.ignored": " ",
    "explorer.git.icon.status.modified": "✹",
    "explorer.git.icon.status.added": "✚",
    "explorer.git.icon.status.untracked": "✭",
    "explorer.git.icon.status.renamed": "➜",
    "explorer.git.icon.status.unmerged": "═",
    "explorer.git.icon.status.deleted": "✖",
    "explorer.git.icon.status.copied": "✔︎",
    "explorer.git.icon.status.unmodified": " ",
    "explorer.git.icon.status.mixed": "*",
    "explorer.file.column.indent.indentLine": false,
    "explorer.keyMappings.global": {
        "<cr>": [
            "expandable?",
            ["expanded?", "collapse", "expand"],
            "open"
        ],
        "h": "gotoParent",
        "l": ["expandable?", "cd", "open"],
        ".": "toggleHidden",
        "R": "refresh",
        "<c-v>": "open:vsplit",
        "<c-t>": "open:tab",
        "af": "addFile",
        "ad": "addDirectory",
        "yp": "copyFilepath",
        "yn": "copyFilename",
        "<c-r>": "rename",
        "<c-c>": "copyFile",
        "<c-x>": "cutFile",
        "<c-p>": "pasteFile",
        "<c-d>": "delete",
        "<c-D>": "deleteForever",
        "q": "quit",
        "<esc>": "esc"
    },
    "explorer.icon.customIcons": {
        "icons": {
            "nodejs": {
                "code": "",
                "color": "#000000"
            },
            "apk": {
                "code": "",
                "color": "#59C369"
            },
            "ipa": {
                "code": "",
                "color": "#D1D5D6"
            }
        },
        "extensions": {
            "apk": "apk",
            "ipa": "ipa"
        },
        "dirnames": {
            "node_modules": "nodejs"
        }
    }
}
